<div class="product-block collection-block" data-collection-id="{{ collection.id }}" {{ custom_attrs }} {% if animate %}data-cc-animate{% endif %}>
  <div class="block-inner">
    <a class="product-link" href="{{ collection.url }}">
      <div class="image-cont" style="border-radius:50%;">
        {%- liquid
          if custom_image
            assign collection_image = custom_image
          elsif collection.featured_image
            assign collection_image = collection.featured_image
          endif
        -%}

        {% if collection_image != blank %}
          <div class="product-block__image product-block__image--primary product-block__image--active">
            {% render 'responsive-image',collection:collection; image: collection_image, manual_image_load: manual_image_load, aspect_ratio: custom_aspect_ratio, cover: true, no_max_width: true %}
          </div>
        {% else %}
          <div class="placeholder-image placeholder-image--shape-{{ settings.coll_grid_image_shape }}">{{ 'collection-1' | placeholder_svg_tag }}</div>
        {% endif %}
        {% if custom_hover_image != blank %}
          <div class="product-block__image product-block__image--primary product-block__image--active hoverimage">
            {% render 'responsive-image', image: custom_hover_image, manual_image_load: manual_image_load, aspect_ratio: custom_aspect_ratio, cover: true, no_max_width: true %}
          </div>
        {% endif %}
        {% comment %}
        <div class="overlay-text overlay-text--for-banner overlay--v-center overlay--h-center image-overlay__over">
          <div class="overlay-text__inner">
            <div class="overlay-text__text collection_itme"><p style="color:#ffff;font-weight:bold;font-size:36px;position: absolute;
    bottom: 0;">{{ collection.title }}</p></div>
          </div>
          </div>
        {% if custom_hover_image != blank %}
          <div class="overlay-text overlay-text--for-banner overlay--v-center overlay--h-center image-overlay__over">
          <div class="overlay-text__inner">
            <div class="overlay-text__text"><p style="color:#ffff;font-weight:bold;font-size:36px;position: absolute;
    bottom: 0;">{{ collection.title }}</p></div>
          </div>
          </div>
        {% endif %}{% endcomment %}
      </div>
      <div class="product-info">
      {% comment %} <div class="product-block__title"><a class="button" href="{{ collection.url }}">SHOP NOW{% comment %}{{ collection.title }}{% endcomment %}</a></div>{% endcomment %}
      <div class="product-block__title">
        <h3>{{ collection.title }}</h3>
        {% if {{ collection.title }} =="PowerPlant Charger" }
           1        
        {% else %} 
        <div class="product-block__recycled" style="color:#73855D;font-weight: bold;">
                   <svg id="Layer_1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320.99 320.99"><defs><style>.cls-1{fill:#5c773e;fill-rule:evenodd;}</style></defs><path class="cls-1" d="M105.21,156.85c15.79,9.73,33.47,28.23,41.26,44.8,1.89,3.67,4.36,1.43,4.86-.05,3.91-7.99,8.54-15.68,13.82-22.82,12.21-16.49,28.01-29.97,47.47-37.05l.56-.21,.61-.04c.62-.04,1.2,.02,1.72,.16,.6,.16,1.13,.42,1.58,.74l.02,.02c.46,.33,.82,.72,1.12,1.13,.3,.43,.5,.89,.64,1.36l.05,.25c.07,.33,.11,.67,.11,1,0,.39-.04,.77-.12,1.12v.02c-.15,.59-.41,1.16-.74,1.63-.34,.51-.79,.95-1.28,1.27-21.22,13.73-41.82,34.89-50.88,58.81-4.04,10.67-3.42,20.1-6.7,28.98-3.52,9.53-13.56,12.34-18.84,2.77-4.81-8.72-4.27-23.03-8.26-34.13-6.71-18.68-18.59-28.9-32.85-42.59-1.59-1.52-2.11-3.9-.91-5.83,.63-1.04,1.64-1.76,2.81-2.06,1.31-.33,2.85,.03,3.95,.72Z"></path><path class="cls-1" d="M162.96,156.24c-1.99,4.25-3.43,7.33-5.34,8.87-1.63,1.76-3.16,.64-3.41-1.38-.41-63.26,70.98-95.83,126.51-88.37,10.86,51.71-25.71,131.83-85.5,119.15-2.27-.56-2.71-3.04-.64-4.25,1.9-1.03,4.18-.81,6.5-1.12,3.94-.53,7.14-1.3,10.86-3.06,41.12-19.4,48.64-53.61,50.44-90.54-44.63,.63-80.41,18.31-99.42,60.7Z"></path><path class="cls-1" d="M114.03,193.19c2.77,.44,2.51,4.14-.04,4.63-59.11,9.77-73.13-39.07-75.63-83.11,41.38-3.03,101.48,.75,105.9,54.55,.3,1.98-1.88,2.95-3.27,1.93-4.8-3.21-7.02-6.91-9.56-11.14-16.27-27.15-48.46-28.63-74.01-27.53,5.41,38.09,18.99,52.84,56.6,60.67Z"></path></svg>
                  100% Compostable
                </div>
        {% endif %}
      </div>
        {% if settings.coll_grid_show_count and collection.all_products_count > 0 %}
          <div class="small">{{ 'collections.general.product_count' | t: count: collection.all_products_count }}</div>
        {% endif %}
      </div>
    </a>
  </div>
</div>